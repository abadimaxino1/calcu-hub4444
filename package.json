{
  "name": "calcu-hub",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite",
    "dev:server": "node server/index.cjs",
    "kill:4000": "powershell -NoProfile -Command \"try { Get-NetTCPConnection -LocalPort 4000 -ErrorAction Stop | Where-Object { $_.OwningProcess -ne 0 } | ForEach-Object { $_.OwningProcess } | Select-Object -Unique | ForEach-Object { Stop-Process -Id $_ -Force } } catch { }\"",
    "stop:server": "npm run kill:4000",
    "restart:server": "npm run kill:4000 && npm run dev:server",
    "test:smoke": "node scripts/test-analytics-api.cjs",
    "test:smoke:auth": "powershell -NoProfile -Command \"if (-not $env:CALCU_ADMIN_COOKIE) { Write-Error 'CALCU_ADMIN_COOKIE is required'; exit 1 }; node scripts/test-analytics-api.cjs\"",
    "test:smoke:ops": "powershell -NoProfile -Command \"if (-not $env:CALCU_ADMIN_COOKIE) { Write-Error 'CALCU_ADMIN_COOKIE is required'; exit 1 }; node scripts/test-admin-ops.cjs\"",
    "test:bool": "node scripts/test-boolean-types.cjs",
    "restart:server:test": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/restart-server-test.ps1",
    "restart:server:test:auth": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/restart-server-test-auth.ps1",
    "start:server": "node server/index.cjs",
    "dev:all": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "build:libs": "node scripts/build-libs.cjs",
    "preview:prod": "vite preview --host",
    "perf:lighthouse:admin": "npx lighthouse http://localhost:4173/admin --only-categories=performance --output=json --output-path=lighthouse-report.json --chrome-flags=\"--headless\"",
    "build": "vite build",
    "preview": "vite preview",
    "build:sw": "node scripts/build-sw.cjs",
    "build:pwa": "npm run build && npm run build:sw",
    "screenshot:dist": "node scripts/screenshot-dist.cjs",
    "build:css": "npx tailwindcss -i ./src/styles/globals.css -o ./src/styles/tailwind-output.css --minify",
    "watch:css": "npx tailwindcss -i ./src/styles/globals.css -o ./src/styles/tailwind-output.css --watch",
    "test:headless": "npm run build:libs && node scripts/run-tests.cjs",
    "seed:users": "node scripts/seed-users.cjs",
    "seed:users:new": "node scripts/seed-users-new.cjs",
    "create:admin": "node scripts/create-admin.cjs",
    "audit:find": "node scripts/find-analytics.cjs",
    "admin:export:analytics": "node -e \"const fs=require('fs');require('child_process').execSync('curl -sS -b \\\"calcu_admin=\\\\\\' -c cookies.txt http://localhost:4000/api/admin/exports/analytics.csv -o audit-artifacts/analytics-export.csv',{stdio:'inherit'})\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "npx tsx scripts/seed-database.ts",
    "verify:all": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/verify-all.ps1"
  },
  "prisma": {
    "seed": "npx tsx scripts/seed-database.ts"
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/client": "^7.0.1",
    "@sentry/node": "^10.32.1",
    "@sentry/react": "^10.32.1",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query": "^5.90.11",
    "@types/dompurify": "^3.0.5",
    "@types/uuid": "^10.0.0",
    "better-sqlite3": "^12.5.0",
    "chart.js": "^4.5.1",
    "compression": "^1.8.1",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "easymde": "^2.20.0",
    "hijri-converter": "^1.1.1",
    "i18next": "^25.6.3",
    "lucide-react": "^0.562.0",
    "marked": "^17.0.1",
    "node-cron": "^4.2.1",
    "prisma": "^7.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.3.5",
    "react-router-dom": "^7.9.6",
    "react-simplemde-editor": "^5.2.0",
    "recharts": "^3.6.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "optionalDependencies": {
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.34.0",
    "autoprefixer": "10.4.22",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "esbuild": "^0.18.20",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.0",
    "jsdom": "^27.2.0",
    "morgan": "^1.10.0",
    "postcss": "8.5.6",
    "puppeteer": "^24.30.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "3.4.7",
    "tsx": "^4.20.6",
    "typescript": "^5.4.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.0",
    "workbox-build": "^6.5.4"
  }
}
